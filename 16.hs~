onesToTeens :: Integer -> String
onesToTeens n
  | n == 0 = ""
  | n == 1 = "one"
  | n == 2 = "two"
  | n == 3 = "three"
  | n == 4 = "four"
  | n == 5 = "five"
  | n == 6 = "six"
  | n == 7 = "seven"
  | n == 8 = "eight"
  | n == 9 = "nine"

tens :: Integer -> String
tens n
  | b < 20 = onesToTeens n
  | b < 30 = "twenty"
  where b = n `mod` 10